    </style>
    <style name="BaseStripe3ds2ProgressTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:colorBackground">@color/stripe_3ds2_background</item>
        <item name="colorPrimary">@color/stripe_3ds2_primary</item>
        <item name="colorPrimaryDark">@color/stripe_3ds2_primary_dark</item>
        <item name="colorAccent">@color/stripe_3ds2_accent</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="android:textColor">@color/stripe_3ds2_text_color</item>
        <item name="android:textColorPrimary">@color/stripe_3ds2_text_color_primary</item>
    </style>
    <style name="Stripe3DS2ActionBar" parent="Widget.AppCompat.ActionBar.Solid">
        <item name="contentInsetStart">@dimen/stripe_3ds2_challenge_activity_padding</item>
        <item name="background">?attr/colorPrimary</item>
        <item name="android:background">?attr/colorPrimary</item>
    </style>
    <style name="Stripe3DS2ActionBarButton" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:minWidth">0dp</item>
    </style>
    <style name="Stripe3DS2Button" parent="Widget.MaterialComponents.Button">
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Stripe3DS2Divider">
        <item name="android:background">@color/stripe_3ds2_divider</item>
        <item name="android:layout_height">@dimen/stripe_3ds2_divider</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    <style name="Stripe3DS2Divider.Vertical">
        <item name="android:layout_width">@dimen/stripe_3ds2_divider</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="Stripe3DS2EditText" parent="BaseStripe3DS2EditText"/>
    <style name="Stripe3DS2FooterChevron">
        <item name="tint">@color/stripe_3ds2_chevron</item>
    </style>
    <style name="Stripe3DS2FooterHeader">
        <item name="android:textColor">@color/info_zone_header_text</item>
    </style>
    <style name="Stripe3DS2FooterText"/>
    <style name="Stripe3DS2FullScreenDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>
    <style name="Stripe3DS2HeaderTextViewStyle">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Stripe3DS2TextButton" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Stripe3DS2TextInputLayout" parent="BaseStripe3DS2TextInputLayout"/>
    <style name="Stripe3DS2TextViewStyle">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Stripe3DS2Theme" parent="BaseStripe3DS2Theme"/>
    <style name="Stripe3ds2ProgressTheme" parent="BaseStripe3ds2ProgressTheme"/>
</resources>